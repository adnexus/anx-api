{
  "name": "anx-api",
  "version": "5.0.5",
  "description": "AppNexus Api Wrapper",
  "homepage": "https://github.com/appnexus/anx-api",
  "bugs": "https://github.com/appnexus/anx-api/issues",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/appnexus/anx-api.git"
  },
  "scripts": {
    "build": "babel src -d lib",
    "cover": "jest src --coverage",
    "dependency-check": "nsp check",
    "lint": "eslint src --quiet",
    "lint-fix": "eslint src --fix",
    "preversion": "npm run build",
    "test": "jest src",
    "watch": "jest src --watch"
  },
  "pre-push": [
    "lint",
    "test",
    "dependency-check"
  ],
  "keywords": [
    "AppNexus"
  ],
  "author": "Tim Santeford",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "axios": "^0.15.0",
    "lodash": "^4.17.4",
    "qs": "^6.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.6.0",
    "coveralls": "^2.11.16",
    "eslint": "^3.16.1",
    "eslint-config-appnexus": "^1.6.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-lodash": "^2.3.5",
    "eslint-plugin-promise": "^3.4.2",
    "jest": "^21.0.2",
    "nock": "^9.0.7",
    "nsp": "^2.6.2",
    "pre-push": "^0.1.1",
    "sinon": "^1.17.7",
    "sinon-as-promised": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
